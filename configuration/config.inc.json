{
  "domains": [
    "getinvolved.local",
    "getinvolved.scot",
    "www.getinvolved.scot",
    "getinvolved.org.uk",
    "www.getinvolved.org.uk"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/getinvolved/",
    "menus": "{:./data/menus.json}",
    "elasticsearchUrl": "{$./data/elasticsearch-url.txt}",
    "termTypeLabels": {
      "activities": {
        "singular": "Activity",
        "plural": "Activities"
      },
      "client_groups": {
        "singular": "Client group",
        "plural": "Client groups"
      }
    },
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    }
  }
}